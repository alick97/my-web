{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@xz-ui/*": ["packages/xz-ui/*"],
      "@@/*": ["src/*"],
    }
  },
  "include": [
    "vite.config.ts",
    "packages/**/*.ts", "packages/**/*.d.ts", "packages/**/*.tsx", "packages/**/*.vue", "src/**/*.ts", "src/**/*.vue"
  ]
}
